{
  "name": "contracts",
  "version": "0.1.0",
  "description": "Cybertino Interactive NFT contracts",
  "main": "index.js",
  "repository": "git@github.com:cybertino/contracts.git",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy": "npx hardhat deploy",
    "deploy:rinkeby": "npx hardhat deploy --network rinkeby",
    "deploy:bsc-testnet": "npx hardhat deploy --network bsc-testnet",
    "deploy:bsc": "npx hardhat deploy --network bsc",
    "etherscan:rinkeby": "hardhat etherscan-verify --network rinkeby && hardhat etherscan-verify-proxy --network rinkeby",
    "etherscan:bsc-testnet": "hardhat etherscan-verify --network bsc-testnet && hardhat etherscan-verify-proxy --network bsc-testnet",
    "etherscan:bsc": "hardhat etherscan-verify --network bsc && hardhat etherscan-verify-proxy --network bsc",
    "console": "npx hardhat console --no-compile"
  },
  "author": "Ryan Li <r@cybertinolab.com>",
  "license": "MIT",
  "devDependencies": {
    "@ethersproject/bignumber": "^5.2.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts-upgradeable": "^4.1.0",
    "@openzeppelin/hardhat-upgrades": "^1.8.2",
    "@typechain/ethers-v5": "^7.0.0",
    "@typechain/hardhat": "^2.0.1",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.3.1",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.1.4",
    "hardhat": "^2.3.0",
    "hardhat-deploy": "^0.7.9",
    "module-alias": "^2.2.2",
    "prettier": "^2.3.0",
    "prettier-plugin-solidity": "^1.0.0-beta.10",
    "qs": "^6.10.1",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^5.0.0",
    "typescript": "^4.2.4",
    "web3": "^1.3.6"
  },
  "_moduleAliases": {
    "@utils": "utils",
    "@typechain": "typechain",
    "@artifacts": "artifacts"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.1.0"
  }
}
